<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:web='urn:import:geogebra.web.gui.app'>
	<ui:style>
		.debugborder { border:1px solid black;}
	  
	</ui:style>
	<g:SplitLayoutPanel ui:field='ggwSplitLayoutPanel'>
        <g:west size='300'>
          <web:GGWViewWrapper addStyleNames="{style.debugborder}" ui:field='ggwViewWrapper' />
        </g:west>

        <g:center>
          <web:GGWGraphicsView ui:field='ggwGraphicsView' />
        </g:center>
        
      </g:SplitLayoutPanel>
</ui:UiBinder> 