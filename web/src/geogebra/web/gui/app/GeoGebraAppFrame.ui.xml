<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:web='urn:import:geogebra.web.gui.app'>
  
   <ui:style>
	  .debugborder { border:1px solid black;}
  </ui:style>

  <g:DockLayoutPanel addStyleNames="{style.debugborder}" unit='PX'>
    <g:north size='50'>
      <web:GGWToolBar addStyleNames="{style.debugborder}" ui:field='ggwToolBar' />
    </g:north>
    
    <g:south size='50'>
          <web:GGWCommandLine ui:field='ggwCommandLine' />
    </g:south>
  </g:DockLayoutPanel>

</ui:UiBinder>