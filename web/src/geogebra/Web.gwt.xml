  <!-- default internationalization language -->
  <set-property-fallback name="locale" value="en"/>
  
  <extend-configuration-property name="compiler.splitpoint.initial.sequence"
    value="geogebra.common.main.settings.Settings"/>
  <extend-configuration-property name="compiler.splitpoint.initial.sequence"
    value="geogebra.web.io.MyXMLio"/>
  <extend-configuration-property name="compiler.splitpoint.initial.sequence"
    value="geogebra.common.main.FontManager"/>

  
  <!-- For login to Google -->
  <inherits name="com.google.api.gwt.oauth2.OAuth2" />
  
  <!-- Images, js-es will be copied from here.
  You can reference them by GWT.getModuleBaseUrl()+"js/file.js" -->
  <public path="resources" />
  
