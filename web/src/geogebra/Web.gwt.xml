<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='web'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <!-- Priority matters! Latter definitions override former (even in former xml-s -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.resources.Resources" />
   <!-- Other module inherits        -->
    <!-- GeoGebraTube upload -->
  <inherits name="com.google.gwt.http.HTTP"/>  
  <inherits name='com.google.gwt.xml.XML' /> 
  <inherits name='geogebra.Common'/>
  <!-- CAS -->
  <inherits name='org.mathpiper.MpreduceJs' />
  
 
   
   

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.    -->
  <!--Changed to our Own THEME, that is basically the Clean theme-->
  <inherits name='geogebra.theme.Clean'/>
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  <!--  upBinder for GUI -->
  <inherits name="com.google.gwt.uibinder.UiBinder" />
 
  <!-- Specify the app entry point class.                         -->
  <entry-point class='geogebra.web.Web'/>

  <!-- compile only for Firefox / Chrome / Safari - choose one: reduces permutation time --> 
  <set-property name="user.agent" value="safari, ie9, opera, gecko1_8" />
  <!--<set-property name="user.agent" value="safari, gecko1_8" />-->
  <add-linker name="xsiframe" />
  <!-- Specify the paths for translatable code                    -->
  <source path='web'/>
  <super-source path='/common/src/geogebra/jre' />
  
