<?xml version="1.0" encoding="ISO-8859-1" ?>

<!--
   GeoGebra kickstart build script    
   @author Zoltan Kovacs <zoltan@geogebra.org>
-->

<project default="input3D">

	<!-- these targets should come from the GeoGebra repository (currently) -->
	<target name="input3D">

	<mkdir dir="lib"/>
	<get 
	    src="http://dev.geogebra.org/download/lib/websocket/jetty-io-9.0.5.v20130815.jar"
	    dest="lib/jetty-io.jar" usetimestamp="true"/>
	<get 
	    src="http://dev.geogebra.org/download/lib/websocket/jetty-util-9.0.5.v20130815.jar"
	    dest="lib/jetty-util.jar" usetimestamp="true"/>

	<get 
	    src="http://dev.geogebra.org/download/lib/websocket/websocket-api-9.0.5.v20130815.jar"
	    dest="lib/websocket-api.jar" usetimestamp="true"/>
	<get 
	    src="http://dev.geogebra.org/download/lib/websocket/websocket-client-9.0.5.v20130815.jar"
	    dest="lib/websocket-client.jar" usetimestamp="true"/>
	<get 
	    src="http://dev.geogebra.org/download/lib/websocket/websocket-common-9.0.5.v20130815.jar"
	    dest="lib/websocket-common.jar" usetimestamp="true"/>
	<get 
	    src="http://dev.geogebra.org/download/lib/websocket/websocket-server-9.0.5.v20130815.jar"
	    dest="lib/websocket-server.jar" usetimestamp="true"/>
	<get 
	    src="http://dev.geogebra.org/download/lib/websocket/websocket-servlet-9.0.5.v20130815.jar"
	    dest="lib/websocket-servlet.jar" usetimestamp="true"/>

	<get src="http://dev.geogebra.org/download/lib/pxcclr/libpxcclr.java.jar" dest="lib/libpxcclr.java.jar"
	    usetimestamp="true"/>
	<get src="http://dev.geogebra.org/download/lib/pxcclr/libpxcclr.jni.dll" dest="lib/libpxcclr.jni.dll"
	    usetimestamp="true"/>

	</target>
	
</project>
