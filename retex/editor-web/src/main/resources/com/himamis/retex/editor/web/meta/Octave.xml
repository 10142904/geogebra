<Serializer>
	    <Arrays>
        <Array name="curly">
            <Open cas="{" tex="\lbrace " />
            <Close cas="}" tex="\rbrace " />
            <Field cas="," />
            <Row cas=";" />
        </Array>
        <Array name="regular">
            <Open cas="(" tex="\left( " />
            <Close cas=")" tex="\right) " />
            <Field cas="," />
            <Row cas=";" />
        </Array>
        <Array name="square">
            <Open cas="[" tex="\left[ " />
            <Close cas="]" tex="\right] " />
            <Field cas="," />
            <Row cas=";" />
        </Array>
        
        <Array name="line">
            <Open cas="|" tex="|" />
            <Close cas="|" tex="|" />
            <Field cas="," />
            <Row cas=";" />
        </Array>
       <Array name="apostrophes">
            <Open cas="&quot;" tex=" \text{&quot;" />
            <Close cas="&quot;" tex="&quot;} " />
        </Array>
    </Arrays>

	<Matrix>
