<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='mobile'>
  <inherits name='com.google.gwt.user.User'/>
  <!-- Compile only for Safari now -->
  <set-property name="user.agent" value="safari" />
  <!-- Inherits Web Hopefully int inherits everything that is in it, and even Common-->
  <inherits name="geogebra.Common"/>
  <inherits name='geogebra.Web'/>
  
  <!-- Specify the app entry point class.                         -->
  <entry-point class='geogebra.mobile.Mobile'/>
  <source path='mobile'/>
  

</module>
